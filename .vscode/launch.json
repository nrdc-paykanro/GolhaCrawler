{
    "name": "Launch index.ts",
    "type": "node",
    "request": "launch",
    "runtimeArgs": [
        "-r",
        "ts-node/register"
    ],
    "args": [
        "${workspaceFolder}/src/index.ts"
    ],
    "configurations": [
    {
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "name": "nodemon",
        "program": "${workspaceFolder}/src/index.ts",
        "request": "launch",
        "restart": true,
        "runtimeExecutable": "nodemon",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "node"
    }
    ]
}